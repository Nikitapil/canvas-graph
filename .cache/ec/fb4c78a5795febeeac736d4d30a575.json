\n        'Jul',\n        'Aug',\n        'Sep',\n        'Oct',\n        'Nov',\n        'Dec'\n    ]\n    const date = new Date(timestamp)\n    return `${shortMonth[date.getMonth()]} ${date.getDate()}`\n}\n\nexport function isOver(mouse, x, length, dWidth) {\n    if (!mouse) {\n        return false\n    }\n    const width = dWidth / length\n    return Math.abs(x - mouse.x) < width / 2\n}\n\nexport function line(ctx, coords, {color}) {\n    ctx.beginPath()\n    ctx.lineWidth = 4;\n    ctx.strokeStyle = color\n    for (const [x, y] of coords) {\n        ctx.lineTo(x, y)\n    }\n    ctx.stroke()\n    ctx.closePath()\n}\n\nexport function circle(ctx, [x, y]) {\n    const CIRCLE_RADIUS = 8\n    ctx.beginPath()\n    ctx.arc(x, y, CIRCLE_RADIUS, 0, Math.PI * 2)\n    ctx.stroke()\n    ctx.closePath()\n}\n\nexport function computeBoundaries({columns, types}) {\n    let min;\n    let max;\n\n    columns.forEach(col => {\n        if(types[col[0]] !== 'line') {\n            return\n        }\n\n        if (typeof min !== 'number') min = col[1];\n        if (typeof max !== 'number') max = col[1];\n\n        if (min > col[1]) min = col[1];\n        if (max < col[1]) max = col[1];\n\n        for (let i = 2; i< col.length; i++) {\n            if (min > col[i]) min = col[i];\n            if (max < col[i]) max = col[i];\n        }\n    })\n\n    return [min, max]\n}\n\nexport function css(el, styles = {}) {\n    Object.assign(el.style, styles)\n}"},"lineCount":null}},"error":null,"hash":"0c1150809846175cec5b7d217b7d1053","cacheData":{"env":{}}}